{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 0
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    }
}